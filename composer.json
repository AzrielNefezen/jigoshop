		"symfony/config": "2.4.*",
		"symfony/yaml": "2.4.*",
		"symfony/proxy-manager-bridge": "2.4.*",
		"monolog/monolog": "~1.11@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "4.1.*",
		"mockery/mockery": "0.9.*"
	}
